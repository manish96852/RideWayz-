{
  "version": 2,
  "name": "ridewayz-app",
  "framework": "create-react-app",
  "buildCommand": "cd ridewayz-app && npm install && npm run build",
  "outputDirectory": "ridewayz-app/build",
  "installCommand": "cd ridewayz-app && npm install",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*\\.(js|css|json|ico|png|jpg|jpeg|gif|svg))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}